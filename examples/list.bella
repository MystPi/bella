let
  mylist =
    [1, 2, 3, 4, 5, 6, 7, 8, 9]

  map =
    list -> fn ->
      match list
        is [] then []
        is [first | rest] then
          [fn(first)] + map(rest, fn)

  reduce =
    list -> acc -> fn ->
      match list
        is [] then acc
        is [first | rest] then
          reduce(rest, fn(first, acc), fn)

  fold =
    list -> fn ->
      let [first | rest] = list in
      reduce(rest, first, fn)
in
(
  ; Squares
  map(mylist, x -> x * x) |> Io.println

  ; Sum
  fold(mylist, x -> a -> x + a) |> Io.println

  ; Length
  reduce(mylist, 0, x -> a -> a + 1) |> Io.println
)